{
  "name": "agescxjs",
  "version": "0.0.1",
  "description": "import/export data from Age of Empires 2 scenario .scx files ",
  "main": "dist/index.js",
  "scripts": {
    "test": "webpack --config webpack.test.config.js && jasmine JASMINE_CONFIG_PATH=jasmine.json",
    "dev": "webpack-dev-server --config webpack-dev.config.js",
    "build": "npm run lint:src && npm run build:web && npm run build:node",
    "build:web": "webpack --config webpack.web.config.js",
    "build:node": "webpack --config webpack.node.config.js",
    "lint": "npm run lint:src && npm run lint:test",
    "lint:src": "tslint src/**/*.ts",
    "lint:test": "tslint test/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dderevjanik/AgeScxJS.git"
  },
  "keywords": [
    "agescx",
    "aoe2",
    "ageofempires",
    "ageofempires2",
    "scx"
  ],
  "author": "Daniel Derevjanik",
  "license": "MIT",
  "homepage": "https://github.com/dderevjanik/AgeScxJS#readme",
  "devDependencies": {
    "jasmine": "^2.4.1",
    "typescript": "^1.8.2",
    "ts-loader": "^0.8.1",
    "tslint": "^3.5.0",
    "webpack": "^1.12.13",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "asdata": "git+https://github.com/dderevjanik/asdata.git"
  }
}
